<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.example.harshita.hospitalappointy">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/heart_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/heart_launcher"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        tools:ignore="AllowBackup">
        <activity
            android:name="com.example.harshita.hospitalappointy.SplashActivity"
            android:screenOrientation="portrait"
            tools:ignore="LockedOrientationActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name="com.example.harshita.hospitalappointy.home.HomeActivity"
            android:screenOrientation="portrait"
            tools:ignore="LockedOrientationActivity" />
        <activity
            android:name="com.example.harshita.hospitalappointy.auth.LoginActivity"
            android:parentActivityName="com.example.harshita.hospitalappointy.home.HomeActivity"
            android:screenOrientation="portrait"
            tools:ignore="LockedOrientationActivity" />
        <activity
            android:name="com.example.harshita.hospitalappointy.auth.RegisterPatientActivity"
            android:parentActivityName="com.example.harshita.hospitalappointy.auth.LoginActivity"
            android:screenOrientation="portrait"
            tools:ignore="LockedOrientationActivity" />
        <activity
            android:name="com.example.harshita.hospitalappointy.patient.PatientViewDoctorProfileActivity"
            android:parentActivityName="com.example.harshita.hospitalappointy.home.HomeActivity"
            android:screenOrientation="portrait"
            tools:ignore="LockedOrientationActivity" />
        <activity
            android:name="com.example.harshita.hospitalappointy.patient.BookAppointmentActivity"
            android:screenOrientation="portrait"
            android:parentActivityName="com.example.harshita.hospitalappointy.patient.PatientViewDoctorProfileActivity"
            tools:ignore="LockedOrientationActivity" />
        <activity
            android:name="com.example.harshita.hospitalappointy.patient.PatientViewBookedAppointmentActivity"
            android:parentActivityName="com.example.harshita.hospitalappointy.home.HomeActivity"
            android:screenOrientation="portrait"
            tools:ignore="LockedOrientationActivity" />
        <activity
            android:name="com.example.harshita.hospitalappointy.doctor.DoctorProfileActivity"
            android:parentActivityName="com.example.harshita.hospitalappointy.home.HomeActivity"
            android:screenOrientation="portrait"
            tools:ignore="LockedOrientationActivity" />
        <activity
            android:name="com.example.harshita.hospitalappointy.doctor.EditDoctorProfileActivity"
            android:parentActivityName="com.example.harshita.hospitalappointy.doctor.DoctorProfileActivity"
            android:screenOrientation="portrait"
            tools:ignore="LockedOrientationActivity" />
        <activity
            android:name="com.example.harshita.hospitalappointy.doctor.ShowDoctorAppointmentActivity"
            android:parentActivityName="com.example.harshita.hospitalappointy.home.HomeActivity"
            android:screenOrientation="portrait"
            tools:ignore="LockedOrientationActivity" />
        <activity
            android:name="com.example.harshita.hospitalappointy.auth.ForgotPasswordActivity"
            android:parentActivityName="com.example.harshita.hospitalappointy.auth.LoginActivity"
            android:screenOrientation="portrait"
            tools:ignore="LockedOrientationActivity" />
        <activity
            android:name="com.example.harshita.hospitalappointy.feedback.FeedbackActivity"
            android:screenOrientation="portrait"
            tools:ignore="LockedOrientationActivity" />
        <activity
            android:name="com.example.harshita.hospitalappointy.about.AboutActivity"
            android:screenOrientation="portrait"
            tools:ignore="LockedOrientationActivity" />

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />

    </application>

</manifest>